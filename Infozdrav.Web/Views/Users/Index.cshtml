@model object

@{
    ViewBag.Title = "Users";
    Layout = "_Layout";
}

<h2>Users</h2>

<ejs-grid id="grid" dataSource=@ViewBag.DataSource allowPaging="true">
    <e-grid-columns>
        <e-grid-column field="Name" textAlign="Right" width="120"></e-grid-column>
        <e-grid-column field="Email" textAlign="Right" width="150"></e-grid-column>
    </e-grid-columns>
</ejs-grid>